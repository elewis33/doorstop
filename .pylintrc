[MESSAGES CONTROL]

# I0011: locally disabling %s
# W0142: used * or ** magic
# W0511: TODO in comments
# C0103: invalid constant name
# RO904: too many public methods
# E0611: no name in module
# E1101: instance has no attribute with name
# E1102: not callable
# F0401: unable to import
disable=I0011,W0142,W0511,C0103,R0904,E0611,E1101,E1102,F0401

[REPORTS]

reports=no

msg-template={msg_id}:{line:3d},{column}:{msg}

[FORMAT]

max-line-length=79

ignore-long-lines=^.*#+.*((https?:)|(pragma:)|(TODO:)).*$
